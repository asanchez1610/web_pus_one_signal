<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
			<setting name="logImpl" value="LOG4J"/>
			<setting name="cacheEnabled" value="true"/>
	</settings>
	
	<typeAliases>
			<typeAlias type="com.notiificaciones.push.domain.mysql.UsuarioSubscrito" alias="UsuarioSubscrito" />
			<typeAlias type="com.notiificaciones.push.domain.mysql.Usuario" alias="Usuario" />
	</typeAliases>
	
	<mappers>
			<mapper resource="com/notiificaciones/push/dao/mysql/sqlmap/UsuarioSubscritoMapper.xml" /> 
			<mapper resource="com/notiificaciones/push/dao/mysql/sqlmap/UsuarioMapper.xml" /> 
	</mappers> 
	
</configuration>